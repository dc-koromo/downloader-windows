<UserControl x:Class="Koromo_Copy_UX.SettingViewScript"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Koromo_Copy_UX"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Margin="20">
        <StackPanel HorizontalAlignment="Stretch">
            <TextBlock Text="스크립트 정보" FontSize="20"/>
            <TextBlock Text="Koromo Copy에 포함된 스크립트는 누구나 쉽게 제작할 수 있고, 유지보수가 매우 편리한 스크립트 언어입니다. 본 개발자는 이 스크립트 언어의 개발을 통해 여러 다운로더의 개발 시간을 단축시킬 수 있었습니다." TextWrapping="Wrap"/>
            <StackPanel x:Name="ScriptInfo" Margin="0 10 0 0"/>
        </StackPanel>
    </Grid>
</UserControl>
