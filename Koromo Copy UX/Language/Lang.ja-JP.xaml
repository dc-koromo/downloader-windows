    <system:String x:Key="msg_download_metadata">Downloading metadata...</system:String>
    <system:String x:Key="msg_download_hiddendata">Downloading hiddendata...</system:String>
    <system:String x:Key="msg_download_data_complete">Downloaded all of data.!</system:String>
    <system:String x:Key="msg_new_update">There are new updates! Please check in Settings -> Update!</system:String>
    <system:String x:Key="msg_incorret_search_grammar">Search grammar is invalid.</system:String>
    <system:String x:Key="msg_tidy" xml:space="preserve"> items has organized!</system:String>
    <system:String x:Key="msg_download_start" xml:space="preserve"> start downloading...</system:String>
    <system:String x:Key="msg_download_complete">Download complete!</system:String>
    <system:String x:Key="msg_download_pause">Downloading Pause</system:String>
    <system:String x:Key="msg_download_resume">Downloading Resume</system:String>

