<Window x:Class="Koromo_Copy_UX.Utility.ForbiddenTester"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Koromo_Copy_UX.Utility"
        mc:Ignorable="d"
        Title="Koromo Copy Forbidden-Tester" Height="297.701" Width="474.297">
    <Grid Margin="8">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <StackPanel Grid.Row="0" HorizontalAlignment="Left" Orientation="Horizontal">
            <TextBox Grid.Row="0" x:Name="Id" Width="400" Text="1330000-1365000"/>
            <Button Content="Test" Width="50" Click="Button_Click"/>
        </StackPanel>

        <TextBox x:Name="Status" Grid.Row="1"/>

        <Button Content="Merge" Grid.Row="2"/>
    </Grid>
</Window>
