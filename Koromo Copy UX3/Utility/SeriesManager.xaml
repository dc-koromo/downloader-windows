<Window x:Class="Koromo_Copy_UX3.Utility.SeriesManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Koromo_Copy_UX3.Utility"
        xmlns:domain="clr-namespace:Koromo_Copy_UX3.Domain"
        xmlns:controls="clr-namespace:Koromo_Copy_UX3.Controls"
        mc:Ignorable="d"
        Title="SeriesManager" Height="508.831" Width="653">
    <Grid>
        <controls:WheelSpeedScrollViewer SpeedFactor="{Binding Source={x:Static Member=domain:SettingWrap.Instance}, Path=SearchSpaceWheelSpeed}">
            <StackPanel x:Name="SeriesPanel" Background="#121212"/>
        </controls:WheelSpeedScrollViewer>
    </Grid>
</Window>
